{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard","prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint","prettier"
    ],
    "rules": {
        "prettier/prettier": "error"
       // "prettier/prettier": ["error", {"singleQuote": true, "parser": "flow"}]

      
    }
}
