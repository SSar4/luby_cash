{
  "compilerOptions": {
    "target": "es2017",  
    "module": "commonjs", 
    "outDir": "./dist",  
    "allowJs": true,   
    "esModuleInterop": true,              
    "forceConsistentCasingInFileNames": true,                                  
    "skipLibCheck": true                                 
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules",
    "data"
  ]
}